SOURCES_SVC.h = 
SOURCES.x = account.x

TARGETS_SVC.c = account_svc.c account_server.c account_xdr.c 
TARGETS_CLNT.c = account_clnt.c account_client.c account_xdr.c 
TARGETS = account.h account_xdr.c account_clnt.c account_svc.c account_client.c account_server.c
REMOVEABLE_TARGET = account.h account_xdr.c account_clnt.c account_svc.c 

OBJECTS_CLNT = $(SOURCES_CLNT.c:%.c=%.o) $(TARGETS_CLNT.c:%.c=%.o)
OBJECTS_SVC = $(SOURCES_SVC.c:%.c=%.o) $(TARGETS_SVC.c:%.c=%.o)
# Compiler flags 

CFLAGS += -g 
LDLIBS += -lnsl
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_CLNT) : $(SOURCES_CLNT.c) $(SOURCES_CLNT.h) $(TARGETS_CLNT.c) 

$(OBJECTS_SVC) : $(SOURCES_SVC.c) $(SOURCES_SVC.h) $(TARGETS_SVC.c) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.c) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.c) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

 clean:
	 $(RM) core $(REMOVEABLE_TARGET) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)
